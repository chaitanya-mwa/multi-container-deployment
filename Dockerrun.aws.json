{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "fhsinchy/notes-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "api",
            "image": "fhsinchy/notes-api",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "fhsinchy/notes-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hosPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "api"]
        }
    ]
}